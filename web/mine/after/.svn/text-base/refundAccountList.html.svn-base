<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>退款清单</title>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="css/all.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="js/jquery.js"></script>
<script type="text/javascript" src="js/base.js"></script>
<script type="text/javascript" src="js/ligerui.all.js"></script>
<script type="text/javascript" src="js/datepicker.js"></script>
<script type="text/javascript" src="js/common.js"></script>
<script src="js/json2.js" type="text/javascript"></script>
<script type="text/javascript" src="refundAccountList.js"></script>
</head>
<body class="mine">
	<div class="i-wrap">
	  <div class="trade_search">
          <table width="100%" class="search_grid">
            <tr>
                <td class="title" width="10%">标的编号：</td>
                <td width="15%">
                	<input type="text" id="no" class="i-input_w160 i-input_bg"/>
                </td>
                <td class="title" width="10%">中心开户行：</td>
                <td width="15%">
                	<input type="text" id="accountBank" class="i-input_w160 i-input_bg"/>
                </td>
                <td class="title" width="10%">中心账号：</td>
                <td width="38%">
                	<input type="text" id="accountNo" class="i-input_w160 i-input_bg"/>
                </td>
            </tr>
            
            <tr>
                <td class="title">交款人户名：</td>
                <td>
                	<input type="text" id="bidderAccountName" class="i-input_w160 i-input_bg"/>
                </td>
                <td class="title">入账类型：</td>
                <td>
                	<select id="billType" class="i-input_w160 i-select_bg">
                       <option value="">--请选择--</option>
                       <option value="0">其它款</option>			
                       <option value="1">保证金</option>			
                       <option value="2">服务费</option>
                       <option value="3">收成交款</option>
                       <option value="4">退成交款</option>
                       <option value="5">暂交款</option>
                       <option value="6">错转款</option>
                    </select>
                </td>
                <td class="title">交易日期：</td>
                <td>
                	<input type="text" id="txtBeginDate" class="i-input_w160 i-input_bg"/> 至 <input type="text" class="i-input_w160 i-input_bg" id="txtEenDate"/>
                </td>
            </tr>
            <tr>
            	<td colspan="6" class="tr pr5 pb5">
                	<span class="wr_btn" id="btnQuery"><input type="button" value="查询" class="o_btn" /></span>
                    <span class="wr_btn" id="btnReset"><input type="button" value="重置" class="o_btn" /></span>
                   
                </td>
            </tr>
        </table>
	    </div>
	  <div id="refundAccountGrid" class="trade_panel"></div>
	</div>
</body>
</html>
