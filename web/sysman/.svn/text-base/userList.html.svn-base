<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>用户管理</title>
<link href="css/all.css" rel="stylesheet" type="text/css" />
<script src="js/jquery.js" type="text/javascript"></script>
<script src="js/base.js" type="text/javascript"></script>
<script src="js/ligerui.all.js" type="text/javascript"></script>
<script src="js/common.js" type="text/javascript"></script>
<script src="js/ca.js" type="text/javascript"></script>
<script type="text/javascript" src="userList.js"></script>
</head>
<body>
<div class="trade_search mb5">
  <table width="100%" class="search_grid">
    <tr>
      <td class="title" style="width:10%">用户名：</td>
      <td width="20%"><input type="text" id="txtUserName" class="i-input_bg" style="width:91%"/></td>
      <td class="title" style="width:10%">类型：</td>
      <td width="30%"><select id="cboUserType" class="i-select_bg" style="width:74%">
          <option value=""></option>
          <option value="0">内部人员</option>
          <option value="1">竞买人</option>
          <option value="2">银行工作人员</option>
        </select>
      </td>
      <td class="title" style="width:10%">状态：</td>
      <td width="20%"><select id="cboStatus" class="i-select_bg" style="width:95%">
          <option value=""></option>
          <option value="0">申请中</option>
          <option value="1">正常</option>
          <option value="2">已过期</option>
          <option value="-1">无效</option>
        </select>
      </td>
    </tr>
    <tr>
      <td class="title" style="width:10%"><span id="selectModeSection"></span>是否申请CAKEY：</td>
      <td><select name="cboHasCakey" id="cboHasCakey" class="i-select_bg" style="width:95%">
          <option value=""></option>
          <option value="1">已申请CA KEY</option>
          <option value="0">未申请CA KEY</option>
        </select>
      </td>
      <td class="title" style="width:10%">CAKEY：</td>
      <td><input type="text" id="txtCakey" class="i-input_bg" style="width:71%"/>
          <input class="i-btn2" type="button" id="btnReadCakey" value="读取"/></td>
      <td class="title" style="width:10%">黑白名单</td>
      <td>
      	  <select name="cboHasCakey" id="rbuserType" class="i-select_bg" style="width:95%">
      	  	<option value=""></option>
         	<option value="100">白名单</option>
          	<option value="200">黑名单</option>
      	  </select>
      </td>
    </tr>
    <tr>
    	<td colspan="6" class="tr"><span class="wr_btn" id="btnQuery">
        <input type="button" value="查询" class="o_btn" />
        </span> <span class="wr_btn" id="btnAdd" style="display:none">
        <input type="button" value="新增" class="o_btn" />
        </span> 
        <span class="wr_btn" id="btnWhite" style="display:none">
        <input type="button" value="加入白名单" class="o_btn" />
        </span>
        <span class="wr_btn" id="btnBlack" style="display:none">
        <input type="button" value="加入黑名单" class="o_btn" />
        </span>
         <span class="wr_btn" id="btnView" style="display:none">
        <input type="button" value="查看变更历史" class="o_btn" />
        </span>
        <span class="wr_btn" id="btnDelete">
        <input type="button" value="删除" class="o_btn" />
        </span> &nbsp;</td>
    </tr>
  </table>
</div>
<div id="userGrid" class="trade_panel"></div>
</body>
</html>
