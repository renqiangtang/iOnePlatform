<body>
<div class="i-wrap">
  <div class="trade_search mb5">
    <form id="decodeOfferLogForm" encType="multipart/form-data">
      <table width="100%" class="search_grid">
        <tr>
          <td class="title">出价记录：</td>
          <td><input type="file" name="file" class="i-input_bg" style="width:90%; background:#fff"></td>
          <td><span class="wr_btn">
            <input type="button" class="o_btn" value="解密文件" onclick="pageDecodeOfferLogListObj.uploadFile()"/>
            </span> <span class="wr_btn">
            <input type="button" class="o_btn" value="重写" onclick="pageDecodeOfferLogListObj.reset()">
            </span> </td>
        </tr>
      </table>
      <iframe name='hidden_frame' id="hidden_frame" style='display:none'></iframe>
    </form>
  </div>
  <div id="offerLogGrid" class="trade_panel"></div>
</div>
