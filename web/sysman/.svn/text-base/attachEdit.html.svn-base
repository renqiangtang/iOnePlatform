<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>附件编辑</title>
<link href="css/all.css" rel="stylesheet" type="text/css" />
<script src="js/jquery.js" type="text/javascript"></script>
<script src="js/base.js" type="text/javascript"></script>
<script src="js/ligerui.all.js" type="text/javascript"></script>
<script src="js/jquery.md5.js" type="text/javascript"></script>
<script type="text/javascript" src="attachEdit.js"></script>
</head>
<body>
<form>
  <table width="100%" class="i-grid-default">
    <tr>
      <td width="120" height="30" class="h_bk s_rb tr"><span class="c6">*</span>附件分类:</td>
      <td class="s_b"><input type="text" id="txtAttachName" class="i-input_off" style="width:90%"/></td>
    </tr>
    <tr>
      <td height="30" class="h_bk s_rb tr">公开:</td>
      <td class="s_b"><input type="checkbox" id="chkPublish" class="ml5"/></td>
    </tr>
    <tr>
      <td height="30" class="h_bk s_rb tr">备注:</td>
      <td class="s_b"><input type="text" id="txtRemark" class="i-input_off" style="width:90%"/></td>
    </tr>
  </table>
  <div class="mt5 pl5 tc" id="action">
  	<span class="wr_btn" id="btnSave">
    <input type="button" value="保存" class="o_btn" />
    </span>
    <span class="wr_btn" id="btnClose">
    <input type="button" value="关闭" class="o_btn" />
    </span>
  </div>
</form>
</body>
</html>
