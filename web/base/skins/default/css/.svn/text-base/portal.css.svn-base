}
.land .portal_search .search_btn {
	background: url("images/base/portal_land.gif") 0 -162px;
	width: 38px;
	height: 22px;
	border: none;
}
.land .home_p {
	background: url("images/base/portal_land.gif") no-repeat -97px -40px;
	width: 33px;
	height: 23px;
}
.land .end_p {
	background: url("images/base/portal_land.gif") no-repeat -97px -63px;
	width: 33px;
	height: 23px;
}
.land .up_p {
	width: 60px;
	height: 23px;
	background: url("images/base/portal_land.gif") no-repeat 0px -138px;
}
.land .next_p {
	width: 60px;
	height: 23px;
	background: url("images/base/portal_land.gif") no-repeat -61px -138px;
}
.land .login_btn {
	background: url("images/base/portal_land.gif");
	width: 128px;
	height: 39px;
	display: inline-block;
}
.page_list a, .page_list i {
	display: inline-block;
}
.land .page_list i {
	font-family: tahoma;
	cursor: pointer;
	font-size: 12px;
	padding: 0px 8px;
	height: 21px;
	line-height: 21px;
	margin-right: 3px;
	text-align: center;
	border: 1px solid #a7bbec;
	background-color: #e9f4fd;
	color: #00379c;
	vertical-align:top;
	font-style:normal;
	
	
}
.land .page_list i.cur {
	color: #1d81cd;
	font-weight: bold;
	border: none;
	background: #ffffff;
}
.l-text-wrapper {
	float: left;
}
.land .portal_search .l-text {
	width: 124px;
}
.land .portal_search .l-text-date input {
	border: none;
}
.land .portal_search .l-text-wrapper .l-text {
	height: 21px;
	line-height: 21px;
}
.land .portal_search .l-trigger, .l-trigger-hover, .l-trigger-pressed {
	margin-top: 1px;
}
.land .portal_search #no input {
	height: 21px;
	line-height: 21px;
}
.land .portal_search .reset_btn {
	background: url("images/base/portal_land.gif") -44px -162px;
	width: 38px;
	height: 22px;
	border: none;
	cursor:pointer;
}
.land .moresearch_b{
background: url("images/mh/l_moresearch.gif") 0 -1px;
height: 22px;
line-height: 21px;
color:#ffffff;
border:1px solid #00b3dd;
display:inline-block;
padding:0 10px;
vertical-align:middle;
}
.floatBtn {
	bottom: 185px;
	left: 50%;
	margin: 0 0 0 530px;
	position: fixed;
	_position:absolute;
	_bottom:auto;
	_top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)));
	_margin-bottom:185px;
	z-index: 11;
}

#scroll_Top {
	background: url("images/mh/floatBtn.jpg") -1px 0;
	margin: 10px 0 0;
	border: 1px solid #DDDDDD;
	border-radius: 3px 3px 3px 3px;
	cursor: pointer;
	height: 58px;
	width: 58px;
}
#scroll_Top:hover {
	background: url("images/mh/floatBtn.jpg") -60px 0;
}


